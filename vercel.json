{
  "version": 2,
  "builds": [
    {
      "src": "myrav/wsgi.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/admin/static/(.*)",
      "dest": "/static/admin/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "myrav/wsgi.py"
    },
    {
      "src": "/googlecc38ee9c840f1e71.html",
      "dest": "/static/googlecc38ee9c840f1e71.html"
    }
  ],
  "buildCommand": "bash ./vercel-build-step.sh"
}